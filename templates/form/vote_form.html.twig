
    <form class="song-vote-form">
        {{ 'Vote this song' }}
        <select id="rating-song-{{ songId}}"  class="rating">
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
            <option>5</option>
            <option>6</option>
            <option>7</option>
            <option>8</option>
            <option>9</option>
            <option>10</option>
        </select>
        <button data-action="click->song-vote#vote" data-url="{{ path('app_songs_vote') }}" data-song="{{ songId }}" data-list="{{ path('app_songs_list') }}"  type="button" class="btn btn-success song-vote-button">{{ 'Send' }}
        </button>
    </form>
