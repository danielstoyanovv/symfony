<form class="song-vote-form">
    {{ 'Vote this song' }}
    <input type="hidden" class="song" value="{{ songId }}">
    <input type="hidden" class="song-vote-action" value="{{ path('app_songs_vote') }}">
    <select class="rating">
        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
        <option>6</option>
        <option>7</option>
        <option>8</option>
        <option>9</option>
        <option>10</option>
    </select>
    <button type="button" class="btn btn-success song-vote-button">{{ 'Send' }}</button>
</form>